{
  "name": "mydevices-cli",
  "version": "1.0.0",
  "description": "CLI tool for managing myDevices IoT platform",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "mydevices": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --compile --outfile dist/mydevices",
    "dev": "bun --watch src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mydevices",
    "iot",
    "cli",
    "cayenne"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^8.0.2",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
